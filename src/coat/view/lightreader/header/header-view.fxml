<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1"
            xmlns="http://javafx.com/javafx/8"
            fx:controller="coat.view.lightreader.header.LightHeaderViewController">
    <center>
        <ScrollPane fitToWidth="true">
            <VBox alignment="TOP_CENTER" spacing="5">
                <Label text="INFO" />
                <TableView fx:id="infoTable"/>
                <Label text="FORMAT" />
                <TableView fx:id="formatTable"/>
                <Label text="Filter" />
                <TableView fx:id="filterTable"/>
                <Label text="contig" />
                <TableView fx:id="contigTable"/>
                <Label text="other" />
                <TableView fx:id="otherTable"/>
                <Label text="id" />
                <TableView fx:id="idTable"/>
            </VBox>
        </ScrollPane>
    </center>
</BorderPane>
